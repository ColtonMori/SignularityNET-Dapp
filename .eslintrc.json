{
  "extends": ["react-app", "eslint:recommended", "plugin:prettier/recommended"],
  "parser": "babel-eslint",
  "plugins": ["react", "prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": false,
        "max-len": { "code": 120, "ignoreUrls": true }
      }
    ],
    "max-len": ["error", { "code": 120, "ignoreUrls": true }],
    "lines-between-class-members": ["error", "always"]
  }
}
